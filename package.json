{
  "name": "furnitureshop",
  "version": "1.0.0",
  "description": "MERN based furniture shopping app",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Srajan1/FurnitureShop.git"
  },
  "author": "srajan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Srajan1/FurnitureShop/issues"
  },
  "homepage": "https://github.com/Srajan1/FurnitureShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
